<mapper namespace="io.cordys.crm.system.mapper.ExtOrganizationConfigDetailMapper">

    <select id="getOrganizationConfigDetail" parameterType="io.cordys.crm.system.domain.OrganizationConfigDetail" resultType="io.cordys.crm.system.domain.OrganizationConfigDetail">
        SELECT * FROM sys_organization_config_detail WHERE config_id = #{configId} limit 1;
    </select>

    <select id="getOrganizationConfigDetails" parameterType="io.cordys.crm.system.domain.OrganizationConfigDetail" resultType="io.cordys.crm.system.domain.OrganizationConfigDetail">
        SELECT * FROM sys_organization_config_detail WHERE config_id = #{configId}
    </select>

</mapper>