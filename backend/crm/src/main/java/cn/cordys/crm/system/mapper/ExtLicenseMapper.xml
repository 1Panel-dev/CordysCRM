<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cordys.crm.system.mapper.ExtLicenseMapper">

    <select id="get" resultType="cn.cordys.crm.system.domain.License">
        SELECT * FROM sys_license
    </select>
    <select id="selectLicenseCode" resultType="cn.cordys.crm.system.domain.License">
        SELECT * FROM sys_license where license_code = #{code}
    </select>

</mapper>