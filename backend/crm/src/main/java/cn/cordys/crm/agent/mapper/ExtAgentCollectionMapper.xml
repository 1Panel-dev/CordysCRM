<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.cordys.crm.agent.mapper.ExtAgentCollectionMapper">

    <delete id="deleteByAgentId" parameterType="java.lang.String">
        delete from agent_collection
        where agent_id = #{agentId}
    </delete>


    <select id="getByUserId" resultType="java.lang.String">
        select agent_id from agent_collection where user_id = #{userId}
    </select>
</mapper>